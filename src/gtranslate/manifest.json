  "name": "Google Translate Always on Top",
  "description": "Site webview always on top windows",
  "version": "1.0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["alwaysOnTopWindows", "webview", "storage"],
